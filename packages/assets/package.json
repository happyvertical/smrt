{
  "name": "@smrt/assets",
  "version": "0.37.0",
  "description": "Asset management system with versioning, metadata, and AI-powered operations for SMRT framework",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build --mode library",
    "build:watch": "vite build --mode library --watch",
    "clean": "rm -rf dist",
    "dev": "vite dev",
    "test": "npx vitest run",
    "test:watch": "npx vitest"
  },
  "dependencies": {
    "@smrt/core": "workspace:*",
    "@smrt/tags": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.3",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "ai",
    "agents",
    "assets",
    "asset-management",
    "versioning",
    "metadata",
    "smrt"
  ],
  "author": "HAVE Team",
  "license": "MIT"
}
